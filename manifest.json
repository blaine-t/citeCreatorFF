{
   "background": {
      "scripts": [
         "background.js"
      ]
   },
   "browser_action": {
      "default_icon": {
         "16": "16.png",
         "32": "32.png",
         "48": "48.png",
         "64": "64.png",
         "128": "128.png",
         "256": "256.png"
      },
      "default_popup": "popup.html",
      "default_title": "Cite Creator"
   },
   "content_scripts": [
      {
         "css": [
            "cite.css"
         ],
         "exclude_matches": [
            "*://*.google.com/*"
         ],
         "js": [
            "date.js",
            "mousetrap.min.js",
            "cite.js"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ],
   "description": "Constructs debate cites from meta tags or page info and copies to clipboard.",
   "icons": {
      "16": "16.png",
      "32": "32.png",
      "48": "48.png",
      "64": "64.png",
      "128": "128.png",
      "256": "256.png"
   },
   "manifest_version": 2,
   "name": "Cite Creator",
   "options_ui": {
      "page": "options.html"
   },
   "permissions": [
      "clipboardWrite",
      "storage"
   ],
   "browser_specific_settings": {
      "gecko": {}
   },
   "version": "1.1.1"
}